{
  "expo": {
    "name": "opencode-mobile",
    "slug": "opencode-mobile",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/opencode-icon.png",
    "userInterfaceStyle": "system",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/opencode-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doza62.opencodemobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.doza62.opencodemobile",
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/opencode-adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2acd63e2-d8ab-4f1d-8270-f4fd80a3667b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2acd63e2-d8ab-4f1d-8270-f4fd80a3667b"
    },
    "platforms": ["ios", "web", "android"],
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./assets/opencode-icon.png",
          "color": "#000000",
          "defaultChannel": "default",
          "vapidPublicKey": "YOUR_VAPID_PUBLIC_KEY_HERE"
        }
      ]
    ]
  }
}
